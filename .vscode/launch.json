    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) 启动",
            "type": "cppdbg",
            "request": "launch",
            "program": "/root/synviasat-tests/ltlfsyn",
            "args": ["/root/tools/synviasat-raw/rome/nim_1/nim_1_1.ltlf","/root/tools/synviasat-raw/rome/nim_1/nim_1_1.part", ">", "/root/synviasat-tests/nim_1_1.log"],
            "stopAtEntry": false,
            "cwd": "/root/synviasat-tests/",
            "envFile": "${workspaceFolder}/.env",
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}